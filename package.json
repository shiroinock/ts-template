    "format": "biome format --write .",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.15.1"
}
